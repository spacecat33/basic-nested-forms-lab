<%= form_for @recipe do |f| %>
    <%= f.label :Recipe_Title %>
    <%= f.text_field :title %><br><br>
  
  
    <%= f.fields_for :ingredients do |ingr| %>
      <%= ingr.label :name %>
      <%= ingr.text_field :name %><br>
  
      <%= ingr.label :quantity %>
      <%= ingr.text_field :quantity %><br><br>
  
     
      <% end %><br>

    <%= f.submit %>

    
  <% end %>
  